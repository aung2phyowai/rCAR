[Common Infos]
Codepage=UTF-8
DataFile=Sample data BVA_Multiplexed.eeg
MarkerFile=Sample data BVA_Multiplexed.vmrk
DataFormat=BINARY
; Data orientation: VECTORIZED=ch1,pt1, ch1,pt2..., MULTIPLEXED=ch1,pt1, ch2,pt1 ...
DataOrientation=MULTIPLEXED
